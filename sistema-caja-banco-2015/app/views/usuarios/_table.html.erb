 <div class="col-sm-12 col-md-12">
        <table id="table" class="table table-striped table-hover">
          <thead>
            <tr>
              <th>Nombre</th>
              <th>Apellido</th>
              <th>Correo</th>
              <th></th>
            </tr>
          </thead>

          <tbody>
            <% @usuarios.each do |usuario| %>
              <tr>
                <td><%= usuario.username %></td>
                <td><%= usuario.apellido %></td>
                <td><%= usuario.email %></td>
                <td class="row">
                  <%= link_to edit_usuario_path(usuario), class: 'btn btn-default', title: 'Editar', 'data-toggle' => 'tooltip', 'data-placement' => 'rigth' do %>
                  <span class= "glyphicon glyphicon-pencil"></span>
                  <% end %>
                </td>
              </tr>
            <% end %>
          </tbody>
        </table>

        <br>
      </div>
   