<h1>Listing cupon_emitidos</h1>

<table>
  <thead>
    <tr>
      <th>Nro cupon</th>
      <th>Monto</th>
      <th>Fecha</th>
      <th>Tarjeta</th>
      <th>Concepto</th>
      <th>Proveedor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cupon_emitidos.each do |cupon_emitido| %>
      <tr>
        <td><%= cupon_emitido.nro_cupon %></td>
        <td><%= cupon_emitido.monto %></td>
        <td><%= cupon_emitido.fecha %></td>
        <td><%= cupon_emitido.tarjeta %></td>
        <td><%= cupon_emitido.concepto %></td>
        <td><%= cupon_emitido.proveedor %></td>
        <td><%= link_to 'Show', cupon_emitido %></td>
        <td><%= link_to 'Edit', edit_cupon_emitido_path(cupon_emitido) %></td>
        <td><%= link_to 'Destroy', cupon_emitido, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cupon emitido', new_cupon_emitido_path %>
