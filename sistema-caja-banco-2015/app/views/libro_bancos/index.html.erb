<h1>Listing libro_bancos</h1>

<table>
  <thead>
    <tr>
      <th>Cuenta</th>
      <th>Fecha inicio</th>
      <th>Fecha fin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @libro_bancos.each do |libro_banco| %>
      <tr>
        <td><%= libro_banco.cuenta_id %></td>
        <td><%= libro_banco.fecha_inicio %></td>
        <td><%= libro_banco.fecha_fin %></td>
        <td><%= link_to 'Show', libro_banco %></td>
        <td><%= link_to 'Edit', edit_libro_banco_path(libro_banco) %></td>
        <td><%= link_to 'Destroy', libro_banco, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Libro banco', new_libro_banco_path %>
