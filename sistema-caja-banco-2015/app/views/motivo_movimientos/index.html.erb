<h1>Listing motivo_movimientos</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @motivo_movimientos.each do |motivo_movimiento| %>
      <tr>
        <td><%= motivo_movimiento.descripcion %></td>
        <td><%= link_to 'Show', motivo_movimiento %></td>
        <td><%= link_to 'Edit', edit_motivo_movimiento_path(motivo_movimiento) %></td>
        <td><%= link_to 'Destroy', motivo_movimiento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Motivo movimiento', new_motivo_movimiento_path %>
