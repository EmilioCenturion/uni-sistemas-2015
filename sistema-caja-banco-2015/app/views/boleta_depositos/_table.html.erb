<div class="col-sm-12 col-md-12">
  <table id="table" class="table table-striped table-hover">
    <thead>
      <tr>
        <th>Nro de boleta</th>
        <th>Nro de cuenta</th>
        <th>Monto en efectivo</th>
        <th>Monto en cheque</th>
        <th>Fecha</th>
      </tr>
    </thead>

    <tbody>
      <% @boleta_depositos.each do |boleta| %>
        <tr>
          <td><%= boleta.nro_boleta %></td>
          <% cuenta = Cuentum.find(boleta.cuenta_id) %>
          <td><%= cuenta.nro_cuenta %></td>
          <td><%= boleta.monto_efectivo %></td>
          <td><%= boleta.monto_cheque %></td>
          <td><%= boleta.fecha %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
