<h1>Listing configuracions</h1>

<table>
  <thead>
    <tr>
      <th>Nombre empresa</th>
      <th>Direccion</th>
      <th>Correo</th>
      <th>Telefono</th>
      <th>Logo empresa</th>
      <th>Cabecera impresion</th>
      <th>Tiempo espera</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @configuracions.each do |configuracion| %>
      <tr>
        <td><%= configuracion.nombre_empresa %></td>
        <td><%= configuracion.direccion %></td>
        <td><%= configuracion.correo %></td>
        <td><%= configuracion.telefono %></td>
        <td><%= configuracion.logo_empresa %></td>
        <td><%= configuracion.cabecera_impresion %></td>
        <td><%= configuracion.tiempo_espera %></td>
        <td><%= link_to 'Show', configuracion %></td>
        <td><%= link_to 'Edit', edit_configuracion_path(configuracion) %></td>
        <td><%= link_to 'Destroy', configuracion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Configuracion', new_configuracion_path %>
